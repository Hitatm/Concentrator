CREATE TABLE topo
      (topoid integer primary key autoincrement, 
       synlevel int, 
       synsqnum int, 
       syntimestamp time, 
       restarttimes int,
       rtx int,
       cpu bigint,
       num_of_neighbour int,
       BeaconInterval int,
       lpm bigint,
       syntimediff int,
       voltage float,
       ParentID varchar,
       transmit bigint,
       realtimestamp varchar,
       nodetimestamp time,
       ID varchar,
       synparentID varchar,
       listen bigint);
